<h4 class='h3 mb-3 font-weight-normal text-center mt-5'>History of Address for:
 <%= AddressBook.find(params[:address_book_id]).fullname %></h4>
  
<table class="table">
  <tr>
    
    <th> Address</th>
    <th>City </th>
      <th> Anniversary</th>
      
  </tr>
 
  <% @logs.each do |address| %>
    <tr>
      <td><%= address.type_property%></td>
      <td><%= address.address%></td>
      <td><%= address.city%></td>
      <td><%= address.anniversary.to_s(:long)%>
       
      
      
    </tr>
    
  <% end %>
  
</table>
<%= link_to 'Home', address_books_path %>