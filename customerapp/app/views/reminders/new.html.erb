<h1>New reminder record</h1>
 <%= form_with scope: :reminder, model: @reminder, local: true do |form| %>
 
  

  

 
  <p>
    <%= form.label :close_type %><br>
    <%= form.text_field :close_type %>
  </p>
 
  <p>
    <%= form.label :periods_remind %><br>
    <%= form.text_area :periods_remind %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
 
<% end %>
 
<%= link_to 'Back', reminders_path %>

